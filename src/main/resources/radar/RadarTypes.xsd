						<xs:enumeration value="Computer Science"/>
						<xs:enumeration value="Economics"/>
						<xs:enumeration value="Engineering"/>
						<xs:enumeration value="Environmental Science and Ecology"/>
						<xs:enumeration value="Ethnology"/>
