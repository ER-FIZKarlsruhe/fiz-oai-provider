						<xs:enumeration value="Horticulture"/>
						<xs:enumeration value="Information Technology"/>
						<xs:enumeration value="Linguistics"/>
						<xs:enumeration value="Life Science"/>
						<xs:enumeration value="Materials Science"/>
						<xs:enumeration value="Mathematics"/>
						<xs:enumeration value="Medicine"/>
						<xs:enumeration value="Philosophy"/>
						<xs:enumeration value="Physics"/>
						<xs:enumeration value="Psychology"/>
						<xs:enumeration value="Social Sciences"/>
						<xs:enumeration value="Software Technology"/>
