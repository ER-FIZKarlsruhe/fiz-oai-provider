<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://radar-service.eu/schemas/descriptive/radar/v09/radar-types" xmlns:dc="http://purl.org/dc/elements/1.1/"
	targetNamespace="http://radar-service.eu/schemas/descriptive/radar/v09/radar-types"
	elementFormDefault="unqualified">

	<xs:import namespace="http://purl.org/dc/elements/1.1/"
		schemaLocation="http://dublincore.org/schemas/xmls/qdc/dc.xsd"/>

	<!-- ********************** TYPE DECLARATIONS ********************** -->
	<!-- define possible values for identifierType attribute -->
	<xs:simpleType name="identifierType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="DOI"/>
			<xs:enumeration value="Handle"/>
			<xs:enumeration value="RADAR"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- define possible values for identifier element -->
	<xs:simpleType name="identifierValue">
					<xs:restriction base="xs:token">
						<xs:pattern value="[^/]+/[^/]+"/>
					</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="notNull">
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="year">
					<xs:restriction base="xs:string">
						<xs:pattern value="\d{4}"/>
					</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="yearOrUnknown">
					<xs:restriction base="xs:string">
						<xs:pattern value="\d{4}|\d{4}-\d{4}|unknown"/>
					</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="softwareType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Resource Production"></xs:enumeration>
			<xs:enumeration value="Resource Processing"></xs:enumeration>
			<xs:enumeration value="Resource Viewing"></xs:enumeration>
			<xs:enumeration value="Other"></xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
