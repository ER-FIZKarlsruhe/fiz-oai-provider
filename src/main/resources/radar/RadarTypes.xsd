						<xs:enumeration value="Theology"/>
						<xs:enumeration value="Veterinary Medicine"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
	</xs:simpleType>
	
	<!-- define possible values for rights element -->
	<xs:simpleType name="rightsType">
					<xs:restriction base="xs:string">
						<xs:enumeration value="CC BY 4.0 Attribution"/>
						<xs:enumeration value="CC BY-ND 4.0 Attribution-NoDerivs"/>
						<xs:enumeration value="CC BY-SA 4.0 Attribution-ShareAlike"/>
